CREATE TABLE  "EMPLOYEE" 
   (	"EMPLOYEEID" NUMBER NOT NULL ENABLE, 
	"EMPLOYEENAME" VARCHAR2(4000), 
	"EMPLOYEEROLE" VARCHAR2(4000), 
	"EMPLOYEEPASSWORD" VARCHAR2(4000), 
	 CONSTRAINT "EMPLOYEE_PK" PRIMARY KEY ("EMPLOYEEID") ENABLE
   );
   
   
   CREATE TABLE  "DISH" 
   (	"DISHID" NUMBER, 
	"DISHNAME" VARCHAR2(4000), 
	"ISVEG" VARCHAR2(4000), 
	"AVAILABILITY" NUMBER, 
	"COST" NUMBER, 
	 CONSTRAINT "DISH_PK" PRIMARY KEY ("DISHID") ENABLE
   )
/

   